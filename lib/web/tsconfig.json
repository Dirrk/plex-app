{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "outDir": "./compiled/js"
    },
    "exclude": [
        "./../../../node_modules/*",
        "./../../node_modules/*",
        "./../node_modules/*",
        "./node_modules/*",
        "node_modules/*"
    ]
}
